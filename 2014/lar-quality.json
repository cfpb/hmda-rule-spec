            "then": {
                "if": {
                    "property": "actionTaken",
                    "condition": "in",
                    "values": ["1", "2", "3", "4", "5", "7", "8"]
                },
                "then": {
                    "property": "metroArea",
                    "condition": "call",
                    "function": "isMetroAreaOnRespondentPanel",
                    "args": ["metroArea", "RespondentID"]
